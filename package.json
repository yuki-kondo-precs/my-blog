{
  "name": "my-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:next tsm",
    "dev:next": "next dev",
    "tsm": "typed-scss-modules src --watch --implementation sass --nameFormat none --exportType default --aliasPrefixes.@ src/",
    "build": "next build",
    "start": "next start",
    "lint": "run-s lint:js lint:style",
    "lint:js": "next lint --dir src",
    "lint:js:fix": "run-s next lint --fix lint:style:fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "lint:style": "stylelint '**/*.{css,scss,sass}'",
    "lint:style:fix": "stylelint --fix '**/*.{css,scss,sass}'"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@next/mdx": "^13.1.6",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "dayjs": "^1.11.7",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "highlight.js": "^11.7.0",
    "microcms-js-sdk": "^2.3.2",
    "next": "13.1.6",
    "next-mdx-remote": "^4.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-scroll": "^1.8.9",
    "react-syntax-highlighter": "^15.5.0",
    "recoil": "^0.7.6",
    "sass": "^1.58.0",
    "the-new-css-reset": "^1.8.3",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/react-scroll": "^1.8.6",
    "@types/react-syntax-highlighter": "^15.5.6",
    "eslint-config-prettier": "^8.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typed-scss-modules": "^7.0.2"
  }
}
